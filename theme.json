{
  "version": 2,
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "settings": {
    "appearanceTools": true,
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "color": {
      "background": true,
      "defaultPalette": false,
      "defaultGradients": false,
      "defaultDuotone": false,
      "custom": false,
      "customGradient": false,
      "customDuotone": false,
      "duotone": [],
      "gradients": [],
      "link": true,
      "palette": [
        {
          "color": "#ffffff",
          "name": "Foreground",
          "slug": "foreground"
        },
        {
          "color": "#f9fbfc",
          "name": "Background",
          "slug": "background"
        },
        {
          "color": "#ffd500",
          "name": "Primary",
          "slug": "primary"
        },
        {
          "slug": "primary-hover",
          "color": "#FDC500",
          "name": "Primary"
        },
        {
          "color": "#00509d",
          "name": "Secondary",
          "slug": "secondary"
        },
        {
          "slug": "secondary-hover",
          "color": "#003F88",
          "name": "Secondary"
        },
        {
          "color": "#00296b",
          "name": "Heading Color",
          "slug": "heading-color"
        },
        {
          "color": "#1f202c",
          "name": "Text Color",
          "slug": "text-color"
        }
      ],
      "text": true
    },
    "custom": {},
    "dimensions": {
      "minHeight": true
    },
    "position": {
      "sticky": true
    },
    "shadow": {
      "defaultPresets": true
    },
    "useRootPaddingAwareAlignments": true,
    "layout": {
      "contentSize": "800px",
      "wideSize": "1200px"
    },
    "spacing": {
      "blockGap": true,
      "margin": true,
      "padding": true,
      "customSpacingSize": true,
      "units": ["px", "em", "rem", "vh", "vw"],
      "spacingScale": {
        "operator": "*",
        "increment": 1.5,
        "steps": 7,
        "mediumStep": 1.5,
        "unit": "rem"
      },
      "spacingSizes": []
    },
    "typography": {
      "customFontSize": true,
      "dropCap": true,
      "fluid": true,
      "fontFamilies": [
        {
          "fontFamily": "Inter",
          "slug": "inter",
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/inter_normal_400.ttf"]
            },
            {
              "fontFamily": "Inter",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": ["file:./assets/fonts/inter_normal_700.ttf"]
            }
          ]
        }
      ],
      "fontSizes": [
        {
          "fluid": {
            "min": "1rem",
            "max": "1.125rem"
          },
          "size": "1.125rem",
          "slug": "small"
        },
        {
          "fluid": {
            "min": "1.25rem",
            "max": "1.625rem"
          },
          "size": "1.625rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "min": "1.375rem",
            "max": "2.125rem"
          },
          "size": "2.125rem",
          "slug": "large"
        },
        {
          "fluid": {
            "min": "1.75rem",
            "max": "2.625rem"
          },
          "size": "2.625rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "min": "2rem",
            "max": "3.25rem"
          },
          "size": "3.25rem",
          "slug": "xx-large"
        }
      ],
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "lineHeight": true,
      "textColumns": true,
      "textDecoration": true,
      "textTransform": true
    },
    "blocks": {
      "core/paragraph": {
        "border": {},
        "color": {},
        "custom": {},
        "layout": {},
        "spacing": {},
        "typography": {}
      },
      "core/heading": {}
    }
  },
  "styles": {},
  "customTemplates": {},
  "templateParts": {}
}
