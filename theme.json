{
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "color": {
      "defaultPalette": false,
      "defaultGradients": false,
      "defaultDuotone": false,
      "custom": false,
      "customGradient": false,
      "customDuotone": false,
      "palette": [
        {
          "color": "#ffffff",
          "name": "Foreground",
          "slug": "custom-foreground"
        },
        {
          "color": "#f9fbfc",
          "name": "Background",
          "slug": "custom-background"
        },
        {
          "color": "#ffd500",
          "name": "Primary",
          "slug": "custom-primary"
        },
        {
          "slug": "primary-hover",
          "color": "#FDC500",
          "name": "Primary"
        },
        {
          "color": "#00509d",
          "name": "Secondary",
          "slug": "custom-secondary"
        },
        {
          "slug": "secondary-hover",
          "color": "#003F88",
          "name": "Secondary"
        },
        {
          "color": "#00296b",
          "name": "Heading Color",
          "slug": "custom-heading-color"
        },
        {
          "color": "#1f202c",
          "name": "Text Color",
          "slug": "custom-text-color"
        }
      ]
    },
    "layout": {
      "contentSize": "820px",
      "wideSize": "1200px"
    },
    "spacing": {
      "blockGap": true,
      "padding": true,
      "margin": true,
      "spacingScale": {
        "steps": 7
      },
      "spacingSizes": [
        {
          "size": "clamp(1.5rem, 5vw, 2rem)",
          "slug": "30",
          "name": "1"
        },
        {
          "size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
          "slug": "40",
          "name": "2"
        },
        {
          "size": "clamp(2.5rem, 8vw, 4.5rem)",
          "slug": "50",
          "name": "3"
        },
        {
          "size": "clamp(3.75rem, 10vw, 7rem)",
          "slug": "60",
          "name": "4"
        },
        {
          "size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
          "slug": "70",
          "name": "5"
        },
        {
          "size": "clamp(7rem, 14vw, 11rem)",
          "slug": "80",
          "name": "6"
        }
      ],
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "dropCap": false,
      "fluid": true,
      "lineHeight": true,
      "fontFamilies": [
        {
          "fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
          "name": "System Font",
          "slug": "system-font"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontStyle": "normal",
              "fontWeight": "400",
              "src": ["file:./assets/fonts/inter_normal_400.ttf"]
            },
            {
              "fontFamily": "Inter",
              "fontStyle": "normal",
              "fontWeight": "700",
              "src": ["file:./assets/fonts/inter_normal_700.ttf"]
            }
          ],

          "fontFamily": "Inter",
          "slug": "inter"
        }
      ],
      "fontSizes": [
        {
          "fluid": {
            "min": "1rem",
            "max": "1.125rem"
          },
          "size": "1.125rem",
          "slug": "small"
        },
        {
          "fluid": {
            "min": "1.25rem",
            "max": "1.625rem"
          },
          "size": "1.625rem",
          "slug": "medium"
        },
        {
          "fluid": {
            "min": "1.375rem",
            "max": "2.125rem"
          },
          "size": "2.125rem",
          "slug": "large"
        },
        {
          "fluid": {
            "min": "1.75rem",
            "max": "2.625rem"
          },
          "size": "2.625rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "min": "2rem",
            "max": "3.25rem"
          },
          "size": "3.25rem",
          "slug": "xx-large"
        }
      ]
    }
  },
  "styles": {
    "blocks": {
      "core/heading": {
        "color": {
          "text": "var:preset|color|custom-heading-color"
        },
        "elements": {
          "link": {
            ":hover": {
              "color": {
                "text": "var:preset|color|custom-secondary"
              }
            },
            "color": {
              "text": "var:preset|color|custom-heading-color"
            }
          }
        }
      },
      "core/image": {
        "border": {
          "radius": "6px"
        },
        "filter": {
          "duotone": "unset"
        }
      },
      "core/list": {
        "typography": {
          "fontSize": "var:preset|font-size|small",
          "textDecoration": "none"
        }
      },
      "core/list-item": {
        "css": "ul {\n list-style: none;\n}",
        "typography": {
          "textDecoration": "none",
          "textTransform": "none"
        }
      },
      "core/navigation": {
        "typography": {
          "fontSize": "var:preset|font-size|medium"
        }
      },
      "core/navigation-link": {
        "typography": {
          "fontSize": "var:preset|font-size|small"
        }
      },
      "core/paragraph": {
        "color": {
          "text": "var:preset|color|custom-text-color"
        },
        "elements": {
          "link": {
            ":hover": {
              "color": {
                "text": "var:preset|color|custom-secondary"
              }
            },
            "color": {
              "text": "var:preset|color|custom-text-color"
            }
          }
        },
        "typography": {
          "fontFamily": "var:preset|font-family|inter",
          "fontSize": "var:preset|font-size|small"
        }
      },
      "core/quote": {
        "typography": {
          "fontSize": "var:preset|font-size|medium"
        },
        "variations": {
          "plain": {
            "typography": {
              "fontSize": "var:preset|font-size|small"
            }
          }
        }
      },
      "core/site-tagline": {
        "color": {
          "text": "var:preset|color|custom-text-color"
        }
      },
      "core/site-title": {
        "color": {
          "text": "var:preset|color|custom-heading-color"
        },
        "elements": {
          "link": {
            ":hover": {
              "color": {
                "text": "var:preset|color|custom-secondary"
              }
            },
            "color": {
              "text": "var:preset|color|custom-heading-color"
            }
          }
        },
        "typography": {
          "fontSize": "var:preset|font-size|medium",
          "textDecoration": "none"
        }
      },
      "jetpack/business-hours": {
        "color": {
          "background": "var:preset|color|custom-background",
          "text": "var:preset|color|custom-text-color"
        }
      },
      "jetpack/contact-form": {
        "color": {
          "background": "var:preset|color|custom-background",
          "text": "var:preset|color|custom-text-color"
        },
        "variations": {
          "animated": {
            "color": {
              "background": "var:preset|color|custom-background",
              "text": "var:preset|color|custom-text-color"
            }
          },
          "outlined": {
            "color": {
              "background": "var:preset|color|custom-background",
              "text": "var:preset|color|custom-text-color"
            }
          }
        }
      },
      "jetpack/contact-info": {
        "color": {
          "text": "var:preset|color|custom-text-color"
        },
        "elements": {
          "link": {
            ":hover": {
              "color": {
                "text": "var:preset|color|custom-secondary"
              }
            },
            "color": {
              "text": "var:preset|color|custom-heading-color"
            }
          }
        }
      }
    },
    "color": {
      "background": "var:preset|color|custom-background",
      "text": "var:preset|color|custom-text-color"
    },
    "elements": {
      "button": {
        "color": {
          "background": "var:preset|color|custom-primary",
          "text": "var:preset|color|custom-heading-color"
        },
        "typography": {
          "fontFamily": "var:preset|font-family|inter",
          "fontSize": "15px",
          "fontStyle": "normal",
          "fontWeight": "700"
        }
      },
      "caption": {
        "color": {
          "text": "var:preset|color|custom-secondary"
        },
        "typography": {
          "fontFamily": "var:preset|font-family|inter"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var:preset|font-size|x-large",
          "textTransform": "uppercase"
        }
      },
      "h2": {
        "typography": {
          "fontSize": "var:preset|font-size|large"
        }
      },
      "h3": {
        "typography": {
          "fontSize": "var:preset|font-size|medium"
        }
      },
      "h4": {
        "typography": {
          "fontSize": "var:preset|font-size|small"
        }
      },
      "heading": {
        "color": {
          "text": "var:preset|color|custom-heading-color"
        },
        "typography": {
          "fontFamily": "var:preset|font-family|inter",
          "fontStyle": "normal",
          "fontWeight": "700"
        }
      },
      "link": {
        ":hover": {
          "color": {
            "text": "var:preset|color|custom-secondary"
          }
        },
        "color": {
          "text": "var:preset|color|custom-text-color"
        },
        "typography": {
          "fontFamily": "var:preset|font-family|inter",
          "fontSize": "var:preset|font-size|small"
        }
      }
    },
    "typography": {
      "fontFamily": "var:preset|font-family|inter",
      "fontSize": "var:preset|font-size|small"
    }
  },
  "templateParts": [
    {
      "area": "header",
      "name": "header"
    },
    {
      "area": "footer",
      "name": "footer"
    }
  ],
  "version": 2,
  "$schema": "https://schemas.wp.org/trunk/theme.json"
}
